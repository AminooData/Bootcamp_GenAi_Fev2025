keys = ["CA", "NJ", "RI"]
values = ["California", "New Jersey", "Rhode Island"]

state_dict = dict(zip(keys, values))  # Associer avec zip() et convertir en dictionnaire
print(state_dict)

# ğŸ“Œ On crÃ©e une liste contenant les voyelles
vowels = ["a", "e", "i", "o", "u"]

# ğŸ“Œ On crÃ©e un dictionnaire vide
vowel_dict = {}

# ğŸ“Œ On remplit le dictionnaire avec les voyelles comme clÃ©s et 0 comme valeur
for letter in vowels:  # ğŸ”„ On parcourt chaque voyelle
    vowel_dict[letter] = 0  # ğŸ“Œ On ajoute {voyelle: 0} au dictionnaire

# ğŸ“Œ On affiche le dictionnaire
print(vowel_dict)  # {'a': 0, 'e': 0, 'i': 0, 'o': 0, 'u': 0}

import string  # ğŸ“Œ On importe la bibliothÃ¨que qui contient l'alphabet

# ğŸ“Œ On rÃ©cupÃ¨re toutes les lettres en minuscules
letters = string.ascii_lowercase  # "abcdefghijklmnopqrstuvwxyz"

# ğŸ“Œ On crÃ©e un dictionnaire vide
alphabet_dict = {}

# ğŸ“Œ On associe chaque lettre Ã  sa position dans l'alphabet
for index, letter in enumerate(letters, start=1):  # ğŸ”„ On compte Ã  partir de 1
    alphabet_dict[index] = letter  # ğŸ“Œ {1: 'a', 2: 'b', ...}

# ğŸ“Œ On affiche le dictionnaire
print(alphabet_dict)
